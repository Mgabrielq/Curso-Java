
public class ListaAlunos {
	
	static final int QUANTIDADE_LISTA = 2;
	
	Aluno[] lista = new Aluno[QUANTIDADE_LISTA];
	
	int tamanhoLista =0;
	
	Aluno obter(int indice) {
		return lista[indice];
	}
	
	int tamanho() {
		return tamanhoLista;
		
	}
	
 	
	void adicionar (Aluno aluno) {
		lista[tamanhoLista] = aluno;
		
		tamanhoLista++;
	}
}
